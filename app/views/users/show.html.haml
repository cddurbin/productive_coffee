= image_tag(@user.profile_pic.thumb)
%h1= @user.first_name + ' ' + @user.last_name
%p= @user.bio
%ul 
  %li= @user.occupation
  %li= @user.email
= link_to('Edit Profile', edit_user_registration_path)

%h2 Establishments added:
- @user.establishments.each do |establishment|
  %h3= link_to(establishment.name)

