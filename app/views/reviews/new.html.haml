= form_for [@establishment, @review] do |f|
  = f.hidden_field :establishment_id, value: @establishment.id
  = f.hidden_field :user_id, value: '1'
  -# = f.hidden_field :user_id, value: current_user.id
  = f.label 'Power'
  = f.select :power, [1, 2, 3, 4, 5]
  = f.label 'Wifi'
  = f.select :wifi, [1, 2, 3, 4, 5]
  = f.label 'Coffee'
  = f.select :coffee, [1, 2, 3, 4, 5]
  = f.label 'Food'
  = f.select :food, [1, 2, 3, 4, 5]
  = f.label 'Service'
  = f.select :service, [1, 2, 3, 4, 5]
  = f.label 'Noise Level'
  = f.select :noise_level, [1, 2, 3, 4, 5]
  = f.label 'Content'
  = f.text_area :content
  = f.submit

